<%= render :partial => "mei/form3", :locals => {:f => f, :key=>key} %>

<% if f.object.class.multiple? key %>
    <%= f.input key, as: :lcsh_lookup, input_html: { class: 'form-control', style: 'width:35em;' }, required: f.object.required?(key), readonly: true %>
    <!-- , disabled: true -->
<% else %>
    <%= f.input key, required: f.object.required?(key) %>
<% end %>