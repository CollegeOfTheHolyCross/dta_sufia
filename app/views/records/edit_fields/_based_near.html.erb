<%= render :partial => "mei/form", :locals => {:f => f, :key=>key, :dropdown_opts=>Sufia.config.spatial_dropdown} %>

<% if f.object.class.multiple? key %>
    <%= f.input key, as: :mei_multi_lookup, input_html: { class: 'form-control', style: 'width:35em;' }, required: f.object.required?(key), readonly: true %>
    <!-- , disabled: true -->
<% else %>
    <%= f.input key, required: f.object.required?(key) %>
<% end %>