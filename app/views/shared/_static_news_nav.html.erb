    <% @nav_items.each do |nav|%>
    <li>
      <%=nav.html_safe%>
    </li>
    <% end %>
  </ul>
  </aside>

  <% if @tag_links.present? %>
  <aside id="tags_section" class="widget well widget_tags">		<h2 class="widget-title" style="margin-top:-10px;">Post Tags</h2>		<ul>
    <% @tag_links.each do |tag|%>
